{
  "name": "courses-app",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "build:prod": "webpack --env mode=production",
    "build:dev": "webpack --env mode=development",
    "start:front": "webpack serve --env port=4000 mode=development",
    "start:server": "node ./json-server/server.js",
    "start": "concurrently \"npm run start:front\" \"npm run start:server\"",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "stylelint": "npx stylelint \"**/*.scss\"",
    "stylelint:fix": "npx stylelint \"**/*.scss\" --fix",
    "test": "jest --config ./config/jest/jest.config.ts",
    "sb": "storybook dev -p 6006 -c\"config/storybook\"",
    "sb:build": "storybook build -c\"config/storybook\"",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve",
    "test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
    "test:ui:json:report": "node scripts/generate-visual-json-report.js",
    "test:ui:visual:report": "npx reg-cli --from .loki/report.json --report .loki/report.html",
    "test:ui:report": "npm run test:ui:json:report && npm run test:ui:visual:report",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
